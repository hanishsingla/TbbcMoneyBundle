{
    "name": "symfgenus/money-bundle",
    "type": "symfony-bundle",
    "description": "This is a Symfony bundle that integrates moneyphp/money library (Fowler pattern): https://github.com/moneyphp/money.",
    "keywords": ["money", "currency", "conversion", "symfony4", "symfony5"],
    "homepage": "https://github.com/TheBigBrainsCompany/TbbcMoneyBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Philippe Le Van",
            "homepage": "https://twitter.com/plv"
        },
        {
            "name": "Sebastien Lefebvre"
        }
    ],
    "require": {
        "php": ">=7.0",
        "ext-curl" : "*",
        "symfony/framework-bundle": "^4.0|^5.0",
        "moneyphp/money": "^3.0",
        "symfony/form": "^4.0|^5.0",
        "symfony/twig-bundle": "^4.0|^5.0",
        "symfony/console": "^4.0|^5.0",
        "symfony/dom-crawler": "^4.0|^5.0"
    },
    "require-dev": {
        "symfony/class-loader": "^4.0|^5.0",
        "symfony/yaml": "^4.0|^5.0",
        "symfony/finder": "^4.0|^5.0",
        "symfony/browser-kit": "^4.0|^5.0",
        "beberlei/DoctrineExtensions": "0.3.*",
        "ext-sqlite3": "*",
        "doctrine/doctrine-bundle": "~1.1",
        "doctrine/orm": "~2.3",
        "phpunit/phpunit": "~4|~5",
        "phpunit/dbunit": "~1.4",
        "symfony/phpunit-bridge": "^4.0|^5.0",
        "squizlabs/php_codesniffer": "^2.7",
        "m6web/Symfony2-coding-standard": "^3.3"
    },
    "suggest": {
        "doctrine/doctrine-bundle": "~1.1",
        "doctrine/orm": "~2.3",
        "florianv/exchanger": "Exchanger is a PHP framework to work with currency exchange rates from various services."
    },
    "autoload": {
        "psr-4": { "Tbbc\\MoneyBundle\\": "" },
        "exclude-from-classmap": [ "/Tests/" ]
    }
}
